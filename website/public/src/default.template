            <!-- begin div.user_nav -->
            <div class="user_nav" id="public_nav">
               [
                  <a href="{customVar: login}">Login</a>
                  |
                  <a href="{customVar: signup}">Register</a>
               ]
            </div>
            <div class="user_nav" id="non_public_nav" style="display:none;">
              [
                 <a href="/prefs">Preferences</a>
                 |
                 <a href="/logout">Logout</a>
                 |
                 <a href="/help/index.html" onclick="window.open(this.href, 'help', 'width=640,height=400,scrollbars=yes,resizable=yes,toolbar=no,directories=no,location=no,menubar=no,status=yes');return false;">Help</a>

              ]
            </div>
