         <!-- begin div.site_header -->
         <div id="site_header">

            <!-- begin div.title_badge -->
            <div id="title_badge">


               <img id="gravatar" src="/images/taskthis-logo.jpg" width="50" height="50" alt="TaskTHIS" title="TaskTHIS" align="top"/>

